{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        // {
        //   "type": "shell",
        //   "style": "powerline",
        //   "powerline_symbol": "",
        //   "background": "#003543",
        //   "foreground": "green",
        //   "properties": {
        //     "template": " {{- .UserName -}} ",
        //     "mapped_shell_names": {
        //       "pwsh": "pwsh"
        //     },
        //     "postfix": ""
        //   }
        // },
        {
          "background": "#003543",
          "foreground": "green",
          "powerline_symbol": "",
          "invert_powerline": true,
          "properties": {
            "template": "{{ .UserName }}",
            "prefix": "üíª‚Äç"
          },
          "style": "powerline",
          "type": "session"
        },
        {
          "background": "#0087D8",
          "foreground": "black",
          "powerline_symbol": "ÓÇ∞",
          "properties": {
            "folder_separator_icon": "/",
            "prefix": " üìÇ ",
            "style": "agnoster_short",
            "enable_hyperlink": true,
            "max_depth": 2
          },
          "style": "powerline",
          "type": "path"
        },
        {
          "background": "#d2ff5e",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#ff9248{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#f26d50{{ end }}",
            "{{ if gt .Ahead 0 }}#89d1dc{{ end }}",
            "{{ if gt .Behind 0 }}#f17c37{{ end }}"
          ],
          "foreground": "#193549",
          "powerline_symbol": "ÓÇ∞",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "template": "{{ .HEAD }}{{ if .Staging.Changed }} ÔÅÜ {{ .Staging.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }} ÔÅÑ {{ .Working.String }}{{ end }}{{ if gt .StashCount 0 }} Ôöí {{ .StashCount }}{{ end }}"
          },
          "style": "powerline",
          "type": "git"
        },
        {
          "background": "green",
          "background_templates": [
            "{{ if gt .Code 0 }}red{{ end }}"
          ],
          "foreground": "#242424",
          "powerline_symbol": "ÓÇ∞",
          "properties": {
            "always_enabled": true,
            "template": "{{ if gt .Code 0 }}ÔÄç{{ end }}{{ if eq .Code 0 }}ÔÄå{{ end }}"
          },
          "style": "powerline",
          "type": "exit"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "horizontal_offset": 0,
      "segments": [
        /* {
          "background": "#f36943",
          "background_templates": [
            "{{if eq \"Charging\" .State.String}}#33DD2D{{end}}",
            "{{if eq \"Discharging\" .State.String}}#FFCD58{{end}}",
            "{{if eq \"Full\" .State.String}}#0476d0{{end}}"
          ],
          "foreground": "#242424",
          "invert_powerline": true,
          "powerline_symbol": "ÓÇ≤",
          "properties": {
            "postfix": "Ôäï üîã "
          },
          "style": "powerline",
          "type": "battery"
        }, */
        {
          "background": "#0087D8",
          "foreground": "#fff",
          "invert_powerline": true,
          "powerline_symbol": "ÓÇ≤",
          "properties": {
            "display_mode": "context",
            "display_version": false,
            "display_virtual_env": true,
            "prefix": " üêç "
          },
          "style": "powerline",
          "type": "python"
        },
        {
          "properties": {
            "template": "{{.Context}}<#fff>|</>{{if .Namespace}}{{.Namespace}}{{end}}",
            "prefix": " ",
            "postfix": " "
          },
          "type": "kubectl",
          "style": "powerline",
          "powerline_symbol": "ÓÇ≤",
          "invert_powerline": true,
          "background": "magenta",
          "foreground": "#fff"
        }
        /* {
          "background": "yellow",
          "foreground": "black",
          "invert_powerline": true,
          "powerline_symbol": "ÓÇ≤",
          "properties": {
            "prefix": " Ôôè "
          },
          "style": "powerline",
          "type": "time"
        } */
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#FFD700",
          "properties": {
            "root_icon": "‚ö°"
          },
          "style": "plain",
          "type": "root"
        },
        {
          "foreground": "red",
          "properties": {
            "prefix": "",
            "text": "Ôë†<b>_</b>"
          },
          "style": "plain",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title": true,
  "console_title_style": "template",
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "final_space": false
}
